unit uCadeiaDeResponsabilidade;

interface

type
  TCadeiaDeResponsabilidade = class
  protected
    limite: Integer;
    proximo: TCadeiaDeResponsabilidade;
  public
    um: Integer; final;
    dois: Integer; final;
    tres: Integer; final;
    procedure setProxima(poCadeia: TCadeiaDeResponsabilidade);
    procedure setMensagem(psMensagem: string; pnPrioridade: Integer);
    function getPrefacio: string;
    procedure setPrefacio(psPrefacio: string);
    function getCapitulos: string;
    procedure setCapitulos(psCapitulos: string);
    function getGlossario: string;
    procedure setGlossario(psGlossario: string);
  end;

implementation

end.
