unit uLgDM110;

interface

uses
  uCelularBasico;

type
  TLgDM110 = class(TInterfacedObject, ICelularBasico)
  private
    modelo: string;
    fabricante: string;
  public
    constructor create;
    function getDadosCelularBasico: string;
  end;

implementation

{ TLgDM110 }

constructor TLgDM110.create;
begin
  modelo := 'DM110';
  fabricante := 'LG';
end;

function TLgDM110.getDadosCelularBasico: string;
begin
  Result := 'Fabricante: ' + fabricante + ' Modelo: ' + modelo;
end;

end.
